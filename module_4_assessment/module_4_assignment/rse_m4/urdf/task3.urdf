<?xml version="1.0" ?>
<robot name="SCARA">
  
  <material name="red">
    <color rgba="0.5 0 0 1"/>
  </material>

  <material name="blue">
    <color rgba="0.5 0 0 1"/>
  </material>

  <link name="base_link"> 
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <geometry>
        <box size="0.4 0.4 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.05"/>
      <geometry>
        <box size="0.4 0.4 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.05" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.02" ixy="0" ixz="0" iyy="0.02" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <link name="ball"> 
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>

  <link name="Rwheel"> 
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
      <geometry>
        <cylinder radius="0.1" length="0.01"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
      <geometry>
        <cylinder radius="0.1" length="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.005" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.0008"/>
    </inertial>
  </link>

  <link name="Lwheel"> 
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.005"/>
      <geometry>
        <cylinder radius="0.1" length="0.01"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.005"/>
      <geometry>
        <cylinder radius="0.1" length="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.005" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.0008"/>
    </inertial>
  </link>

  <link name="Rbase"> 
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <mass value="3.0"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.004"/>
    </inertial>
  </link>

  <link name="link1">
    <visual>
      <origin rpy="0 1.57 0" xyz="0.1 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <material name="Cyan">
        <color rgba="0 1.0 1.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 1.57 0" xyz="0.1 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.1 0 0" rpy="0 1.57 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.003" ixy="0" ixz="0" iyy="0.003" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <link name="link2">
    <visual>
      <origin rpy="0 1.57 0" xyz="0.1 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <material name="green">
        <color rgba="0 1.0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 1.57 0" xyz="0.1 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.1 0 0" rpy="0 1.57 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.003" ixy="0" ixz="0" iyy="0.003" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <link name="link3">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <mass value="2.5"/>
      <inertia ixx="0.004" ixy="0" ixz="0" iyy="0.004" iyz="0" izz="0.003"/>
    </inertial>
  </link>

  <link name="gripper">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.05"/>
      <geometry>
        <box size="0.05 0.1 0.1" />
      </geometry>
      <material name="gf">
        <color rgba="1 0.5 0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.05"/>
      <geometry>
        <box size="0.05 0.1 0.1" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.0005" iyz="0" izz="0.0005"/>
    </inertial>
  </link>

<joint name="base_link_ball" type="fixed">
    <origin xyz="0 -0.1 -0.0125" rpy="0.0 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="ball"/>
  </joint>

  <joint name="base_link_Rwheel" type="continuous">
    <origin xyz="0.2 0.1 0.05" rpy="0.0 1.57 0.0"/>
    <parent link="base_link"/>
    <child link="Rwheel"/>
    <axis xyz="0.0 0 1.0"/>
  </joint>


  <joint name="base_link_Lwheel" type="continuous">
    <origin xyz=" -0.2 0.1 0.05" rpy="0.0 -1.57 0.0"/>
    <parent link="base_link"/>
    <child link="Lwheel"/>
    <axis xyz="0.0 0 1.0"/>
  </joint>

 <joint name="base_link_Rbase" type="fixed">
    <origin xyz="0.0 0.0 0" rpy="0.0 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="Rbase"/>
  </joint>



  <joint name="Rbase_link1" type="continuous">
    <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>
    <parent link="Rbase"/>
    <child link="link1"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>


  <joint name="link1_link2" type="continuous">
    <origin xyz="0.2 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>

  <joint name="link2_link3" type="prismatic">
    <origin xyz="0.2 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="link2"/>
    <child link="link3"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit effort="100" velocity="100" lower="-0.2" upper="0.1"/> 
  </joint>
 
 <joint name="link3_gripper" type="fixed">
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="link3"/>
    <child link="gripper"/>
  </joint>



  <gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="gazebo_base_controller">
      <odometry_frame>odom</odometry_frame>
      <commandTopic>cmd_vel</commandTopic>
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <update_rate>15.0</update_rate>

      <left_joint>base_link_Lwheel</left_joint>
      <right_joint>base_link_Rwheel</right_joint>

      <wheel_separation>0.5</wheel_separation>
      <wheel_diameter>0.3</wheel_diameter>
      <max_wheel_acceleration>0.7</max_wheel_acceleration>
      <max_wheel_torque>8</max_wheel_torque>
      <robotBaseFrame>base</robotBaseFrame>
    </plugin>
  </gazebo>
</robot>
